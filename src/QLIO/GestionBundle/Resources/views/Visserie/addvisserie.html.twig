{% extends "::layout.html.twig" %}

{% block title %} Ajouter - Visserie
{% endblock %}

{% block body %} {{ parent() }}

	{{ form_start(form) }}
	<table>
		<tr>
			<td>{{ form_label(form.numVis) }}</td>
			<td>{{ form_widget(form.numVis) }}</td>
			<td>{{ form_errors(form.numVis) }}</td>
		</tr>
		<tr>
			<td>{{ form_label(form.typeVis) }}</td>
			<td>{{ form_widget(form.typeVis,{'attr': {'value':'typeVis'}}) }}</td>
			<td>{{ form_errors(form.typeVis) }}</td>
		</tr>
		<tr>
			<td>{{ form_label(form.libVis) }}</td>
			<td>{{ form_widget(form.libVis) }}</td>
			<td>{{ form_errors(form.libVis) }}</td>
		</tr>
		<tr>
			<td>{{ form_label(form.poidsVis) }}</td>
			<td>{{ form_widget(form.poidsVis) }}</td>
			<td>{{ form_errors(form.poidsVis) }}</td>
		</tr>
		<tr>
			<td>{{ form_label(form.nEmpPick) }}</td>
			<td>{{ form_widget(form.nEmpPick) }}</td>
			<td>{{ form_errors(form.nEmpPick) }}</td>
		</tr>
	</table>
	{{ form_end(form) }}
	
{% endblock %}