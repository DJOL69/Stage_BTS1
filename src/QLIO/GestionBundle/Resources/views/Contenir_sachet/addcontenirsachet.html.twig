{% extends "::layout.html.twig" %}

{% block title %} Ajouter - Contenir-Sachet
{% endblock %}

{% block body %} {{ parent() }}

	{{ form_start(form) }}
	<table>
		<tr>
			<td>{{ form_label(form.refBox) }}</td>
			<td>{{ form_widget(form.refBox) }}</td>
			<td>{{ form_errors(form.refBox) }}</td>
		</tr>
		<tr>
			<td>{{ form_label(form.refSachet) }}</td>
			<td>{{ form_widget(form.refSachet) }}</td>
			<td>{{ form_errors(form.refSachet) }}</td>
		</tr>
		<tr>
			<td>{{ form_label(form.qteSachet) }}</td>
			<td>{{ form_widget(form.qteSachet) }}</td>
			<td>{{ form_errors(form.qteSachet) }}</td>
		</tr>
	</table>
	{{ form_end(form) }}
{% endblock %}